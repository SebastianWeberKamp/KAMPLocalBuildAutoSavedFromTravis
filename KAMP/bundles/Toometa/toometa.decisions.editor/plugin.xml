<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated decisions -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="decisions.presentation.DecisionsModelWizardID"
            name="%_UI_DecisionsModelWizard_label"
            class="decisions.presentation.DecisionsModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/DecisionsModelFile.gif">
         <description>%_UI_DecisionsModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated decisions -->
      <editor
            id="decisions.presentation.DecisionsEditorID"
            name="%_UI_DecisionsEditor_label"
            icon="icons/full/obj16/DecisionsModelFile.gif"
            extensions="decisions"
            class="decisions.presentation.DecisionsEditor"
            contributorClass="decisions.presentation.DecisionsActionBarContributor">
      </editor>
   </extension>

</plugin>
